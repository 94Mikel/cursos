@model DateTime

<h4>
    <!-- MES ANTERIOR -->
    <a asp-route-mes="@ViewBag.mesAnterior" asp-route-ano="@ViewBag.anoAnterior" class="btn btn-secondary">
        <i class="bi-chevron-left"></i>
    </a>

    @Model.ToString("MMM")-@Model.Year

    <!-- MES POSTERIOR -->
    <a asp-route-mes="@ViewBag.mesPosterior" asp-route-ano="@ViewBag.anoPosterior" class="btn btn-secondary">
        <i class="bi-chevron-right"></i>
    </a>
</h4>